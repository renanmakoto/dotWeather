{
  "expo": {
    "name": "dotWeather",
    "slug": "dotWeather",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png", 
    "scheme": "dotweather",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dotextension.dotweather"
    },
    "android": {
      "package": "com.dotextension.dotweather",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "af79633a-8206-406c-a3c9-449d7016abdc"
      }
    }
  }
}
